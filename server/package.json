{
  "name": "api-storytellin.gg",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "yarn": "^0.27.5"
  },
  "scripts": {
    "build": "yarn run clean && babel src --out-dir dist --copy-files",
    "start-local": "NODE_ENV=dev DATABASE=local nodemon src --exec babel-node src",
    "start-test": "NODE_ENV=test DATABASE=test babel-node src",
    "start-beta": "NODE_ENV=dev DATABASE=beta babel-node src",
    "start": "NODE_ENV=production DATABASE=beta node dist",
    "clean": "rm -rf dist",
    "test": "NODE_ENV=test DATABASE=test yarn run db-populate-test && jest",
    "deploy": "now --docker",
    "db-reset-dev": "NODE_ENV=dev DATABASE=dev babel-node src/db/scripts/reinitialize",
    "db-reset-beta": "NODE_ENV=production DATABASE=beta babel-node src/db/scripts/reinitialize",
    "db-reset-test": "NODE_ENV=test DATABASE=test babel-node src/db/scripts/reinitialize",
    "db-reset-local": "NODE_ENV=dev DATABASE=local babel-node src/db/scripts/reinitialize",
    "db-mia-beta": "yarn run db-reset-beta && NODE_ENV=dev DATABASE=beta babel-node src/db/scripts/mia",
    "db-mia-dev": "yarn run db-reset-dev && NODE_ENV=dev DATABASE=dev babel-node src/db/scripts/mia",
    "db-mia-test": "yarn run db-reset-test && NODE_ENV=dev DATABASE=test babel-node src/db/scripts/mia",
    "db-mia-local": "yarn run db-reset-local && NODE_ENV=dev DATABASE=local babel-node src/db/scripts/mia",
    "db-populate-test": "yarn run db-reset-test && NODE_ENV=test DATABASE=test babel-node src/db/scripts/populateTest"
  },
  "dependencies": {
    "apollo-server-express": "^1.1.0",
    "aws-sdk": "^2.94.0",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chalk": "^2.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-graphql": "^0.6.6",
    "google-auth-library": "^0.11.0",
    "googleapis": "^22.2.0",
    "graphql": "^0.10.5",
    "graphql-server-express": "^1.1.0",
    "graphql-tools": "^1.1.0",
    "isomorphic-unfetch": "^2.0.0",
    "jsonwebtoken": "^7.4.2",
    "multer": "^1.3.0",
    "mysql": "^2.14.1",
    "mysql2": "^1.4.2",
    "node-fetch": "^1.7.2",
    "pg": "^7.0.2",
    "rimraf": "^2.6.1",
    "sequelize": "4.22.0",
    "sharp": "^0.18.2",
    "sqlite3": "^3.1.13"
  }
}
